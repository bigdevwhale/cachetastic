{
  "name": "bigdevwhale/cachetastic",
  "description": "A package for caching method results in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Marat Badykov",
      "email": "marat.badykov.555@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/cache": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Cachetastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "mockery/mockery": "^1.6"
  }
}
